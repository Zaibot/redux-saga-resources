{
  "name": "redux-saga-resources",
  "version": "0.2.1",
  "description": "",
  "typings": "node/index.d.ts",
  "main": "node/index.js",
  "esnext:main": "es6/index.js",
  "typescript:main": "src/index.ts",
  "dependencies": {
    "@zaibot/fsa": "^0.1.4",
    "@zaibot/fsa-saga": "^0.1.0",
    "arrayq": "^0.1.14"
  },
  "optionalDependencies": {
    "isomorphic-fetch": "^2.2.1",
    "redux-typescript-actions": "^1.2.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@zaibot/tslint-preset": "^1.0.2",
    "assert": "^1.4.1",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-node": "^5.1.1",
    "co": "^4.6.0",
    "mocha": "^3.5.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "scripts": {
    "prepublish": "npm run lint && npm run build && npm run test",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf es6/* node/*",
    "compile": "npm run compile:ts && npm run compile:node",
    "compile:ts": "tsc -p tsconfig.json",
    "compile:node": "babel es6 --out-dir node --presets node --plugins transform-runtime --copy-files",
    "lint": "tslint -c tslint.json --type-check --project tsconfig.json",
    "lint:fix": "tslint -c tslint.json --type-check --project tsconfig.json --fix",
    "test": "mocha test",
    "watch": "nodemon -w src -w test -e \"ts js\" --exec \"npm run compile && npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zaibot/redux-saga-resources.git"
  },
